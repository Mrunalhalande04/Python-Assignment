import threading

def number(no):
    print("in forward")
    for i in range(1,no+1):
        print(i)

def revnumber(no):
    print("in backward")
    for i in range(no,0,-1):
        print(i)


def main():
   
   print("Enter number")
   no=int(input())
   
   t1=threading.Thread(target=number,args=(no,))
   t2=threading.Thread(target=revnumber,args=(no,))
   
   
   t1.start()
   t1.join()

   t2.start()
   t2.join()
   

if __name__ =="__main__":
    main()